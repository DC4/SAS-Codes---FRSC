%MACRO DEC(CNTY);

DATA FALT003;
	SET CREDIT.FALT003_&CNTY._CR_&MNTH1.(WHERE=(STRIP(SUBSTR(CLIENT_XID,LENGTH(CLIENT_XID)-4))="&CNTY._CR" AND 
										UPCASE(SUBSTR(CREATED_DTTM,4,6)) IN ("&MNTH_1." "&MNTH_2." "&MNTH_3." ) ))
		CREDIT.FALT003_&CNTY._CR_&MNTH2.(WHERE=(STRIP(SUBSTR(CLIENT_XID,LENGTH(CLIENT_XID)-4))="&CNTY._CR" AND 
