		WHERE UPCASE(RULE_NAME_STRG) NOT LIKE ("%_TEST") AND 
			  UPCASE(RULE_NAME_STRG) NOT LIKE ("%_TEST1") AND 
			  UPCASE(RULE_NAME_STRG) NOT LIKE ("%_TEST2") AND ACCT_NBR NE "";

	IF SUBSTR(ACCT_NBR,1,6) IN ('411144'
								'421451'
