)

OR(  CRD_CLNT_ID  NOT IN ("SC_EURONETIN_DB" "SC_EURONETMY_DB" "SC_EURONETSG_DB" "SC_EURONETAE_DB") AND
(
(COMPRESS(MER_ID) = "145456887" AND  USD  >= 40) OR
(COMPRESS(MER_ID) = "24633695" AND  USD  >= 40) OR
(COMPRESS(MER_ID) = "4556359848" AND  USD  >= 20) OR
(COMPRESS(MER_ID) = "4556487655" AND  USD  >= 45) OR
(COMPRESS(MER_ID) = "24633695" AND  FRD_SCOR  > 300) OR
(COMPRESS(MER_ID) = "000174241204992" AND  USD  > 40)
)
)

OR(
 CRD_CLNT_ID  = "SC_EURONETBH_DB" AND
(
(COMPRESS(MER_ID) = "000445120538993" AND SIC_CD IN ("5399" "5691")) OR
(COMPRESS(MER_ID) = "475700000000001" AND  FRD_SCOR  > 800)
)
