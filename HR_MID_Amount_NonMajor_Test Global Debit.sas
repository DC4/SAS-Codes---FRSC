DATA HR_MID_Amount_NonMajor_TestSAS;
SET FALT002_2;
IF
(
(
 TRN_AUTH_POST  = "A" AND
 AUTH_DECISION_XCD  = "A" AND
 TRN_TYP  IN ("C" "M" "P")
)
and
(
(
 CRD_CLNT_ID  = "SC_SPARROWKE_DB" AND
(
(COMPRESS(MER_ID) = "KA2DJIBSBZMSEA9" AND  USD  >= 40) OR
(COMPRESS(MER_ID) = "100000000007434" AND  USD  >= 10)
